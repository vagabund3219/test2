{
  "version": 2,
  "builds": [
    {
      "src": "kursovoiProekt/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLamdaSize" : "15mb",
        "memory": "512mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/kursovoiProekt/wsgi.py"
    }
  ]
}